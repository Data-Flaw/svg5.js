{
  "name": "svg5",
  "version": "1.0.0",
  "description": "svg5 is a tiny JS library to generate static SVGs based on Processing/p5js syntax.",
  "homepage": "https://github.com/MAKIO135/svg5.js#readme",
  "author": "makio135",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.2.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "rollup": "^3.9.1"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c -w --bundleConfigAsCjs",
